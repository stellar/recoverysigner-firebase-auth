#: src/components/ConfirmVerificationEmail.tsx:40
#: src/components/SendVerificationCode.tsx:46
#: src/components/SendVerificationEmail.tsx:31
msgid "Error: {0}"
msgstr "Error: {0}"

#: src/components/SentVerificationEmail.tsx:15
msgid "If the below email address is associated with an account, you will receive a verification email."
msgstr "Si la dirección de email, aquí debajo, está asociada con una cuenta, vas a recibir un email de verificación."

#: src/helpers/getFirebaseError.ts:27
#: src/helpers/getFirebaseError.ts:32
msgid "Oops! Please try again in a few minutes. (error code: {0})"
msgstr "¡Ups! Por favor intentá nuevamente en unos minutos. (código de error: {0})"

#: src/components/SentVerificationEmail.tsx:22
msgid "Please check your email and click the verification link in the email message to proceed."
msgstr "Por favor revisá tu correo y hacé clic en el link de verificación para proceder."

#: src/helpers/getFirebaseError.ts:17
msgid "Please fill out the captcha to proceed."
msgstr "Por favor completá la verificación para proceder."

#: src/helpers/getFirebaseError.ts:12
msgid "Please provide a valid phone number."
msgstr "Por favor proporcioná un número de teléfono válido."

#: src/components/ConfirmVerificationEmail.tsx:35
#: src/components/SendVerificationCode.tsx:41
#: src/components/SendVerificationEmail.tsx:26
msgid "Please wait…"
msgstr "Por favor esperá…"

#: src/components/ConfirmVerificationCode.tsx:54
#: src/components/SentVerificationEmail.tsx:32
msgid "Resend"
msgstr "Reenviar"

#: src/components/SendVerificationCode.tsx:52
msgid "Send verification code"
msgstr "Enviar código de verificación"

#: src/helpers/getFirebaseError.ts:9
msgid "The confirmation code you provided is invalid."
msgstr "El código de confirmación que proporcionaste no es válido."

#: src/helpers/getFirebaseError.ts:21
msgid "There was a network connectivity issue. Please try again."
msgstr "Hubo un problema de conectividad de red. Intentalo de nuevo."

#: src/helpers/getFirebaseError.ts:19
msgid "This email is already in use. Please use another one."
msgstr "Este correo electrónico ya está en uso. Por favor usá otro."

#: src/components/ConfirmVerificationCode.tsx:41
msgid "We’ve sent you a verification code:"
msgstr "Te hemos enviado un código de verificación:"

#: src/components/ConfirmVerificationCode.tsx:34
#: src/components/ConfirmVerificationEmail.tsx:28
msgid "You’ve been verified! Please wait."
msgstr "¡Has sido verificado! Por favor espera."

#: src/helpers/getFirebaseError.ts:14
msgid "You’ve tried to do that too many times. Please wait a while and try again!"
msgstr "Lo intentaste muchas veces. ¡Por favor, esperá un momento e intentá otra vez!"
